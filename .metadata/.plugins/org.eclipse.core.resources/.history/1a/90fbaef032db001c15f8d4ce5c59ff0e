package season.summer.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class HomeController {

	
	@GetMapping("/")
	public String home() {
		return "welcome";
	}
	@GetMapping("/beach/List")
	public String beachList() {
		return "beachList";
	}
	@GetMapping("/")
	public String home() {
		return "welcome";
	}
	@GetMapping("/")
	public String home() {
		return "welcome";
	}
}
